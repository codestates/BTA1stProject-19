{
  "name": "Velas Wallet",
  "options_page": "options.html",
  "background": {
    "page": "background.html",
    "script": ["background.bundle.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "velas-icon.png",
      "34": "velas-icon-34.png",
      "128": "velas-icon-128.png"
    }
  },
  "icons": {
    "16": "velas-icon.png",
    "34": "velas-icon-34.png",
    "128": "velas-icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["storage", "activeTab"]
}
